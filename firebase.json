{
  "hosting": {
    "public": "www",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/webpushr-sw.js",
        "destination": "/webpushr-sw.js"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
